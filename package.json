{
  "name": "@wenhaoming/rc-keepalive",
  "version": "0.0.1",
  "description": "react-component keepalive",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "react",
    "keeyalive",
    "hooks"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run build-lib",
    "build-lib": "cross-env BUILD_TYPE=lib father-build && cross-env BUILD_TYPE=es father-build",
    "dev": "vite serve demo",
    "build:demo": "tsc && vite build",
    "serve": "vite preview",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot",
    "tsc": "tsc --noEmit",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./src && npm run lint:prettier",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"./src\" --end-of-line auto"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wen-haoming/rc-keepalive.git"
  },
  "peerDependencies": {
    "react": ">=16.9.0"
  },
  "gitHooks": {
    "pre-commit": "pretty-quick --staged",
    "commit-msg": "node ./node_modules/@umijs/fabric/dist/verifyCommit.js"
  },
  "author": "haoming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wen-haoming/rc-keepalive/issues"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "homepage": "https://github.com/wen-haoming/rc-keepalive#readme",
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@umijs/fabric": "^2.5.13",
    "@umijs/test": "^3.4.23",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "father-build": "^1.19.6",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "typescript": "^4.3.2",
    "umi-test": "^1.9.6",
    "vite": "^2.3.5"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
